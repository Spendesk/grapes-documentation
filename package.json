{
  "name": "grapes-documentation",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "precommit": "lint-staged",
    "generate:props": "node ./scripts/docgen.mjs",
    "create:component": "plop component --plopfile scripts/plopfile.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@dev-spendesk/grapes": "^1.55.0",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "15.5.4",
    "@react-docgen/cli": "^3.0.3",
    "@shikijs/transformers": "^3.13.0",
    "@types/mdx": "^2.0.13",
    "github-slugger": "^2.0.0",
    "hastscript": "^9.0.1",
    "libphonenumber-js": "^1.12.22",
    "next": "15.5.4",
    "react": "19.1.1",
    "react-canvas-confetti": "^2.0.7",
    "react-docgen": "^8.0.1",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "19.1.1",
    "remark-directive": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "shiki": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.5.2",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "lodash.kebabcase": "^4.1.1",
    "plop": "^4.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "browserslist": [
    "chrome 120",
    "firefox 121",
    "safari 17.2"
  ],
  "engines": {
    "node": ">= 22"
  },
  "lint-staged": {
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --quiet"
    ]
  }
}
